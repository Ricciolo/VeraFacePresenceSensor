	</actionList>
	<serviceStateTable>
		<stateVariable>
			 <sendEventsAttribute>no</sendEventsAttribute>
			 <name>TimeWindow</name>
			 <dataType>string</dataType>
			 <defaultValue>5</defaultValue>
		</stateVariable>		
		<stateVariable>
			 <sendEventsAttribute>no</sendEventsAttribute>
			 <name>Present</name>
			 <dataType>string</dataType>
			 <defaultValue>no</defaultValue>
